<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:clickable="true"
    android:focusableInTouchMode="true"
    tools:context="com.example.annamiyajima.mapstest.MainActivity">

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint = "Resturant Name"
        android:id="@+id/findeditText" />

    <Button
        style="@style/myButtons"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Find a Restaurant"
        android:id="@+id/findbutton"
        android:layout_gravity="center_horizontal"
        android:layout_below="@+id/findeditText"
        android:layout_alignStart="@+id/searchbutton" />

    <Button
        style="@style/myButtons"
        android:layout_height="wrap_content"
        android:text="Search Near By"
        android:id="@+id/searchbutton"
        android:layout_gravity="center_horizontal"
        android:layout_below="@+id/findbutton"
        android:layout_centerHorizontal="true" />

    <Button
        style="@style/myButtons"
        android:layout_height="wrap_content"
        android:text="Add a Time"
        android:id="@+id/addtimebutton"
        android:layout_alignParentBottom="true"
        android:layout_below="@+id/searchbutton" />

</RelativeLayout>
